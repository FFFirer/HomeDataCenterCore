@using HomeDataCenterCore.Models
@model BodyDataViewModel

<tr">
    <th>
        @Model.RecordTime.ToString("yyyy-MM-dd HH:mm:ss")
    </th>
    <th>
        @Model.Shape
    </th>
    <th>
        @Model.Weight
    </th>
    <th>
        @Model.BMI
    </th>
    <th>
        @Model.BodyFatPercentage
    </th>
    <th>
        @Model.BodyMoisture
    </th>
